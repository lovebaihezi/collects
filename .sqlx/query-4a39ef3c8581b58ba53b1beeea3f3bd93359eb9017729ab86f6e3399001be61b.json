  "query": "\n            UPDATE tags\n            SET\n                name = COALESCE($3, name),\n                color = CASE WHEN $4 THEN $5 ELSE color END\n            WHERE id = $1 AND user_id = $2\n            RETURNING id, user_id, name, color, created_at\n            ",
