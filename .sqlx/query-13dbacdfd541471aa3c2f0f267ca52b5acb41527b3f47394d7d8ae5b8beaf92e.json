  "query": "\n            SELECT\n                id, user_id, name, description,\n                visibility, status,\n                trashed_at, archived_at, created_at, updated_at\n            FROM content_groups\n            WHERE user_id = $1\n              AND ($2::text IS NULL OR status = $2)\n            ORDER BY created_at DESC\n            LIMIT $3 OFFSET $4\n            ",
